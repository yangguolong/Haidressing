+ (NSString *)StringfromOrderStatus:(YZMOrderStatus)orderStatus
{
    NSString *statusStr;
    switch (orderStatus) {
        case YZMOrderStatusAll:
            statusStr = @"";
            break;
        case YZMOrderStatusUnpaid:
             statusStr = @"待付款";
            break;
        case YZMOrderStatusPaid:
             statusStr = @"待消费";
            break;
        case YZMOrderStatusConsumed:
             statusStr = @"待评价";
            break;
        case YZMOrderStatusRefund:
             statusStr = @"退款";
            break;
        default:
            break;
    }
    return statusStr;
}
